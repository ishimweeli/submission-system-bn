  "scripts": {
    "dev": "tsnd --respawn ./src/server.ts",
    "build": "tsc -p . && npm run sentry:sourcemaps",
    "prestart": "npm run build",
    "start": "node ./dist/src/server.js",
    "postinstall": "husky install",
    "lint": "npx eslint src",
    "lint:fix": "npm lint --fix",
    "prettier": "npx prettier --check src",
    "format": "npx prettier --write tests src",
    "prisma:generate": "npx prisma generate",
    "prepare": "husky install",
    "test": "jest --detectOpenHandles",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "seed": "ts-node src/seeders/seedUp.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org niyonizeye-faustin --project git-inspired-capstone ./dist && sentry-cli sourcemaps upload --org niyonizeye-faustin --project git-inspired-capstone ./dist"
  },
